<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VerticalSeekBar">
        <attr name="dockerUSE" format="reference|boolean" />
        <attr name="dockerID" format="reference" />
        <attr name="dockerMargin" format="reference|integer" />
        <attr name="dockerDirection" format="reference|integer" />
        <attr name="dockerSTEP" format="reference|integer" />
        <attr name="min" format="reference|integer" />
        <attr name="max" format="reference|integer" />
        <attr name="step" format="reference|integer" />
        <attr name="init" format="reference|integer" />
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>
</resources>