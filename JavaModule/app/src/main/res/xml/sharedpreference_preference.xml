<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="메시지 알림">
        <SwitchPreferenceCompat
            app:iconSpaceReserved="false"
            app:key="message"
            app:title="메시지 알림"/>
        <Preference
            app:iconSpaceReserved="false"
            app:fragment="java.module.labyrintos.SharedPreference.SettingSubFragment"
            app:key="keyword_screen"
            app:title="키워드 알림"
            app:summary="사용안함"
            />

        <SwitchPreferenceCompat
            app:iconSpaceReserved="false"
            app:key="sound"
            app:title="소리"/>
        <ListPreference
            app:iconSpaceReserved="false"
            app:entries="@array/array_voice"
            app:entryValues="@array/array_voice"
            app:key="sound_list"
            app:summary="카톡"
            app:title="알림음"
            />
    </PreferenceCategory>
</PreferenceScreen>