<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

        <SwitchPreference
            app:iconSpaceReserved="false"
            app:defaultValue="false"
            app:key="keyword"
            app:summary="등록된 키워드가 포함된 메시지는 채팅방 알림이 꺼져 있어도 푸시 알림을 받게 되어 키워드는 강조되어 보여 집니다."
            app:title="키워드 알림"/>
        <ListPreference
            app:iconSpaceReserved="false"
            app:entries="@array/array_voice"
            app:entryValues="@array/array_voice"
            app:key="keyword_sound_list"
            app:summary="카톡"
            app:title="키워드 전용 알림음"
            app:dependency="keyword"
            />

</PreferenceScreen>